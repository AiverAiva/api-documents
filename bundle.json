    },
    "/wynncraft/guildPrefix": {
      "get": {
        "tags": [
          "Wynncraft"
        ],
        "summary": "Get guild name with its prefix.",
        "description": "as summary\n",
        "operationId": "guildPrefix",
        "parameters": [
          {
            "in": "path",
            "name": "prefix",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "This is a required parameter for getting the corresponding guild name to the prefix"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully getting the corresponding guild name to the prefix.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                },
                "examples": {
                  "Lmi": {
                    "value": "Luminosity"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request, the prefix might not exist.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                },
                "examples": {
                  "Bad_request": {
                    "value": {
                      "error": "Guild not found or other issue(check api.weikuwu.me for more infomation)"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/wynncraft/guildName": {
      "get": {
        "tags": [
          "Wynncraft"
        ],
        "summary": "Check the validity of a guild name or prefix",
        "description": "*very important* since the wynncraft api is case sensitive, please be cautious while using this.\n",
        "operationId": "guildName",
        "parameters": [
          {
            "in": "path",
            "name": "guild",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "*very important* if the guild name was delivered, please be cautious that it is case sensitive"
          }
        ],
        "responses": {
          "200": {
            "description": "Successfully getting the guild name.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                },
                "examples": {
                  "Lmi": {
                    "value": "Luminosity"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad request, the prefix/name might not exist.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                },
                "examples": {
                  "Bad_request": {
                    "value": {
                      "error": "Unknown guild name or prefix, this is case sensitive btw(check api.weikuwu.me for more infomation)"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
